  // ? Edit Global For Vscode
  // Auto Save
  "files.autoSave": "afterDelay",
  // Editor Font Size
  "editor.fontSize": 22,
  // Editor Line Height
  "editor.lineHeight": 35,
  // Editor Format On Save
  "editor.formatOnSave": true,
  // Autoprefixer Format On Save
  "autoprefixer.formatOnSave": true,
  // Editor Font Weight
  "editor.fontWeight": "bold",
  // Enable Word Wrapping
  "editor.wordWrap": "on",
  // Highlight matching brackets
  "editor.matchBrackets": "always",
  // Configure the amount of spaces per tab
  // Tab تكوين مقدار المسافات لكل علامة تبويب عند الضغط على مفتاح
  "editor.tabSize": 2,
  // يمنع المحرر من كشف التباعد تلقائياً عند فتح ملف جديد
  // لتحديد حجم التباعد المناسب "editor.tabSize" بدلاً من ذلك، يتبع المحرر قيمة
  "editor.detectIndentation": false,
  // (Spaces) يحدد ما إذا كان المحرر سيستخدم مسافات
  // (Tab) بدلاً من علامات التباعد
  // في هذا الحالة، سيتم استخدام المسافات Tab عند الضغط على مفتاح
  "editor.insertSpaces": true,
