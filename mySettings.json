  // ? TODO => Css Scss
  // Style For Css & Scss Color Of || Property, Value
  "scss.lint.emptyRules": "warning",
  "scss.lint.universalSelector": "warning",
  "scss.format.enable": true,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "support.type.property-name.css",
          "meta.property-list.scss",
          "variable.scss",
          "variable.parameter.scss",
          "keyword.control.import.include.scss"
        ],
        "settings": {
          "foreground": "#9CDCFE"
        }
      },
      {
        "scope": "meta.function-call.scss",
        "settings": {
          "foreground": "#DCDCAA"
        }
      },
      {
        "scope": "keyword.operator.scss",
        "settings": {
          "foreground": "#CE9178"
        }
      },
      {
        "scope": [
          "meta.selector.scss",
          "entity.other.attribute-name.class.scss"
        ],
        "settings": {
          "foreground": "#D16969"
        }
      },
      {
        "scope": ["constant.numeric.scss"],
        "settings": {
          "foreground": "#B5CEA8"
        }
      }
    ]
  },

