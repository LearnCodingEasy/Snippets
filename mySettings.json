  // ? Them I Used
  "workbench.colorTheme": "Theme",
  "workbench.colorCustomizations": {
    // Active Page Open On Tab
    "tab.activeBackground": "#282c34",
    // Bar Color
    "activityBar.background": "#282c34",
    // Bar Active & Open Color
    "sideBar.background": "#282c34",
    // Set the background color of the editor
    // يحدد لون خلفية نافذة التحرير
    "editor.background": "#252633",
    // Set the text color of the editor
    // يحدد لون النص في نافذة التحرير
    "editor.foreground": "#FFFFFF",
    // Set the color of the cursor in the editor
    // يحدد لون المؤشر في نافذة التحرير.
    "editorCursor.foreground": "#FFFFFF",
    // Set the background color of selected text in the editor
    // يحدد لون الخلفية عند تحديد نص في نافذة التحرير
    "editor.selectionBackground": "#504B65",
    // Set the background color of the current line in the editor
    // يحدد لون الخلفية عند تمييز سطر في نافذة التحرير
    "editor.lineHighlightBackground": "#000000",
    //
    //
    "tree.indentGuidesStroke": "#f8f8f2",
    "editorIndentGuide.activeBackground1": "#f8f8f2",
    "editorIndentGuide.background1": "#44475a"
  },

