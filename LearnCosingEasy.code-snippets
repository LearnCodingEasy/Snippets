      "      // رجع البيانات كا ابجاكت",
      "      return answer.json();",
      "    })",
      "    .then((data) => {",
      "      // Data Api هى البيانات التي تم الحصول عليها من ال",
      "      console.log(",
      "        \"%cData %cApi هى البيانات التي تم الحصول عليها من ال\",",
      "        \"color: red; font-size: 18px\",",
      "        \"color: blue; font-size: 18px\"",
      "      );",
