      "                    <header class=\"title-header\">",
      "                      <h2 :id=\"index\">{{ header }}</h2>",
      "                    </header>",
      "                  </div>",
      "                </div>",
      "              </section>",
      "            </article>",
      "            <aside>",
      "              <div class=\"div-aside\">",
      "                <!-- {{ currentSection }} -->",
      "                <a",
      "                  v-for=\"(header, index) in headers\"",
      "                  :key=\"header\"",
      "                  :href=\"`#${index}`\"",
      "                  :class=\"{ activeAside: index == currentSection }\"",
      "                >",
      "                  {{ header }}",
      "                </a>",
      "              </div>",
      "            </aside>",
      "          </div>",
