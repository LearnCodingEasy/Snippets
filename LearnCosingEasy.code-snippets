      "*/",
      "",
      "async function getApi(url) {",
      "  const response = await fetch(url)",
      "    .then((answer) => {",
      "      // Response & Answer Api هى حالة طلب ال",
      "      console.log(",
      "        \"%cResponse & Answer %cApi هى حالة طلب ال\",",
      "        \"color: red; font-size: 18px\",",
      "        \"color: blue; font-size: 18px\"",
