  "test snippets 1": {
    "prefix": "test snippets 1",
    "body": ["console.log('Code');"],
    "description": "test snippets 1  "
  },
  "test snippets 2": {
    "prefix": "test snippets 2",
    "body": ["console.log(`$1`);"],
    "description": "test snippets 1  "
  },
  "test snippets 3": {
    "prefix": "test snippets 3",
    "body": ["console.log(`$Code`);"],
    "description": "test snippets 1  "
  },
  "test snippets 4": {
    "prefix": "test snippets 4",
    "body": ["console.log(`${1:Code}`);"],
    "description": "test snippets 1  "
  },
  "test snippets 5": {
    "prefix": "test snippets 5",
    "body": ["console.log(`${1:Code}`);", "console.log(`${2:Code}`);"],
