  "scss plugin swiper basic": {
    "prefix": "scss plugin swiper basic",
    "body": [
      "    position: relative;",
      "    min-height: 400px;",
      "    .swiper {",
      "      width: 100%;",
      "      height: 100%;",
      "      .swiper-wrapper {",
      "        height: 100%;",
      "        .swiper-slide {",
      "          height: 100%;",
      "          text-align: center;",
      "          font-size: 18px;",
      "          background: #fff;",
      "          display: flex;",
      "          justify-content: center;",
      "          align-items: center;",
      "          img {",
      "            display: block;",
      "            width: 100%;",
      "            height: 100%;",
      "            object-fit: cover;",
      "          }",
      "          &.swiper-slide-active {",
      "            display: block;",
      "          }",
      "        }",
      "      }",
      "      // Button Left & Right",
      "      .swiper-button-next,",
      "      .swiper-button-prev {",
      "        font-size: 15px;",
      "        color: #fff;",
      "        background: #33333375;",
      "        width: 30px;",
      "        height: 100px;",
      "        line-height: 100px;",
      "        margin-top: -50px;",
      "        text-align: center;",
      "        transition: all 0.4s linear;",
      "        left: -50px;",
      "        opacity: 0.5;",
      "        display: flex;",
      "        justify-content: center;",
      "        align-items: center;",
      "        text-decoration: none;",
      "        font-size: 25px;",
      "        &:hover {",
      "          background: #333;",
      "        }",
      "      }",
      "",
      "      // Button Bottom Click",
      "      .swiper-pagination {",
      "        display: flex;",
      "        justify-content: start;",
      "        align-items: center;",
      "        .swiper-pagination-bullet {",
      "          width: 30px;",
      "          border-radius: 5px;",
      "          &.swiper-pagination-bullet-active {",
      "            background-color: var(--color-first-slider);",
      "          }",
      "        }",
      "      }",
      "      // Auto Play",
      "      .autoplay-progress {",
      "        position: absolute;",
      "        right: 16px;",
      "        bottom: 16px;",
      "        z-index: 10;",
      "        width: 48px;",
      "        height: 48px;",
      "        display: flex;",
      "        align-items: center;",
      "        justify-content: center;",
      "        font-weight: bold;",
      "        color: var(--color-first-slider);",
      "        svg {",
      "          --progress: 0;",
      "          position: absolute;",
      "          left: 0;",
      "          top: 0px;",
      "          z-index: 10;",
      "          width: 100%;",
      "          height: 100%;",
      "          stroke-width: 4px;",
      "          stroke: var(--color-first-slider);",
      "          fill: none;",
      "          stroke-dashoffset: calc(125.6 * (1 - var(--progress)));",
      "          stroke-dasharray: 125.6;",
      "          transform: rotate(-90deg);",
      "        }",
      "      }",
      "      &:hover {",
      "        .swiper-button-next {",
      "          opacity: 1;",
      "          right: 0.5rem;",
      "          left: auto;",
      "        }",
      "        .swiper-button-prev {",
      "          opacity: 1;",
      "          left: 0.5rem;",
      "        }",
      "      }",
      "    }"
    ],
    "description": "scss plugin swiper basic"
  },
